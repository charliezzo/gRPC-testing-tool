{
  "name": "grpcman",
  "version": "1.5.3",
  "private": true,
  "scripts": {
    "start": "npm run electron:serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/logo.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "package:all": "npm run package:win && npm run package:linux && npm run package:mac",
    "package:win": "npm run electron:build -- --win 7z --x64",
    "package:linux": "npm run electron:build -- --linux 7z AppImage --x64",
    "package:mac": "npm run electron:build -- --mac 7z dmg"
  },
  "main": "background.js",
  "dependencies": {
    "@grpc/grpc-js": "^1.3.2",
    "@grpc/proto-loader": "^0.6.2",
    "@vue/eslint-config-standard": "^5.0.1",
    "async": "^1.5.2",
    "core-js": "^2.6.5",
    "dayjs": "^1.8.33",
    "element-ui": "^2.11.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "google-protobuf": "^3.0.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.0",
    "protobufjs": "^6.8.8",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^4.5.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "css-loader": "^3.2.0",
    "electron": "^13.4.0",
    "electron-builder": "^22.8.1",
    "electron-icon-builder": "^1.0.1",
    "electron-rebuild": "^1.11.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^1.0.0",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}