{
  "name": "grpcman",
  "version": "1.5.2",
  "private": true,
  "scripts": {
    "rebuild": "npm install grpc --runtime=electron --target=7.3.2 && npm install --save-dev electron-rebuild && npm install && npm start",
    "start": "npm run electron:serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/logo.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@grpc/proto-loader": "^0.1.0",
    "@vue/eslint-config-standard": "^5.0.1",
    "async": "^1.5.2",
    "core-js": "^2.6.5",
    "dayjs": "^1.8.16",
    "element-ui": "^2.11.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "google-protobuf": "^3.0.0",
    "grpc": "^1.24.2",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0",
    "protobufjs": "^6.8.8",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "css-loader": "^3.2.0",
    "electron": "7.3.2",
    "electron-builder": "^21.2.0",
    "electron-icon-builder": "^1.0.1",
    "electron-rebuild": "^1.8.8",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^1.0.0",
    "vue-cli-plugin-electron-builder": "^1.4.0",
    "vue-cli-plugin-element": "^1.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
